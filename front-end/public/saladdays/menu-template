    <script type="text/javascript">
    var saladDaysRestaurantID = 21;
    var pannelHTML;
    function addLeftSideBar() {
    	var sideBarHTML = '<h2>Categories</h2><div class="panel-group category-products" id="accordian"></div>'
    	$(".left-sidebar").append(sideBarHTML);
    }
	addLeftSideBar();
	var imageURLPrefix = 'http://www.cookedspecially.com';
	sections.forEach(function(section) {
    	var panelListElementsHTML = '';
    	var sectionName = section.name;
    	var trimmedSectionName = sectionName.replace(/\s/g,'');
    	var sectionTitleHTML = '<div class="col-sm-12"><h2 class="title text-center">' + sectionName + '</h2></div>';
		$(".features_items").append(sectionTitleHTML);
		var colValue = 3;
    	section.items.forEach(function(item) {
    		var itemName = item.name;
    		var itemID = item.itemId;
    		var itemPrice = item.price;
    		var itemImageURL = imageURLPrefix + item.imageUrl;
    		var innerHTML =
    		'<div id="div_' + itemID + '" class="item-container col-sm-' + colValue + '">' +
    			'<a name="' + itemName + '"/>' +
		    		'<div class="product-image-wrapper">' +
						'<div class="single-products">' +
							'<div class="productinfo text-center">' +
								'<img src="' + itemImageURL + '" alt="" />' +
								'<h2>₹' + itemPrice + '</h2>' +
								'<p>' + itemName + '</p>' +
								'<a href="#" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"/>Add to cart</a>' +
							'</div>' +
						'</div>' +
					'</div>' +
			'</div>';
    		$(".features_items").append(innerHTML);
    		//CREATE PANEL ELEMENTS LIST
    		panelListElementsHTML += '<li><a id="link_' + itemID + '" href="#' + itemName + '">'+ itemName +'</a></li>';
    	});
    	//CREATE LEFT PANEL
    	pannelHTML =
    	'<div class="panel panel-default">' +
			'<div class="panel-heading">' +
				'<h4 class="panel-title">' +
					'<a data-toggle="collapse" data-parent="#accordian" href="#' + trimmedSectionName + '">' +//To Do : Allow Section HREFS
						'<span class="badge pull-right"><i class="fa fa-plus"></i></span>' +
						sectionName +
					'</a>' +
				'</h4>' +
			'</div>' +
			'<div id="' + trimmedSectionName + '" class="panel-collapse collapse">' +
				'<div class="panel-body">' +
					'<ul>' + panelListElementsHTML + '</ul>' +
				'</div>' +
			'</div>';
		console.log(pannelHTML);
		$("#accordian").append(pannelHTML);
    });
    </script>
